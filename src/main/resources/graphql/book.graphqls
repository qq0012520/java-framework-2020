type BookQuery{
    bookById(id: ID): Book
}

type BookMutation{
    addBook(input: BookInput): Book
    deleteBook(id: ID): Boolean
    updateBook(input: BookInput): Book
}

type Book{
    id: ID
    name: String
    pageCount: Int
    author: Author
}

input BookInput{
    id: ID
    name: String
    pageCount: Int
    authorId: ID
}